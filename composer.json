{
    "name": "clagiordano/weblibs-browserinfo",
    "description": "weblibs-browserinfo is a tool library for easily identify user agent by regular expresison or php browscap backend",
    "type": "library",
    "license": "LGPL-3.0",
    "autoload": {
        "psr-4": {
            "clagiordano\\weblibs\\browserinfo\\": "src/"
        }
    },
    "require": {
        "php": ">=5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload-dev": {
        "psr-4": {
            "clagiordano\\weblibs\\browserinfo\\tests\\": "tests/",
            "clagiordano\\weblibs\\browserinfo\\testdata\\": "testdata/"
        }
    }
}
